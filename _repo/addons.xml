<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.advancedsettings"
       name="Easy Advanced Settings"
       version="0.8.9"
       provider-name="kinkin">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               	library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Create, view, edit and delete your advancedsettings.xml file in a user-friendly gui</summary>
        <description>All settings follow the xbmc.org wiki[CR][CR]With this tool you can create your advanced settings directly from XBMC[CR][CR]If you don't know what a setting does, don't enable it!</description>
        <platform>all</platform>
        <language>en</language>
        <license>GNU GENERAL PUBLIC LICENSE</license>
        <forum></forum>
        <website></website>
        <source></source>
        <email></email>
    </extension>

</addon>

<addon id="plugin.program.jupiteronewizard" name="JupiterOne Wizard" version="0.1.0" provider-name="Kodi4">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">JupiterOne Wizard</summary>
    <description lang="en">JupiterOne Wizard allows you to get access to the latest Kodi4 Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>

    <addon id="repository.jupiter1wiz" name="Jupiter One" version="0.1.0" provider-name="Kodi4">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Jupiter One">
            <info compressed="false">https://raw.githubusercontent.com/ringling123/jupiter1wiz/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/ringling123/jupiter1wiz/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/ringling123/jupiter1wiz/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Jupiter One Build Wizard</summary>
            <description>Jupiter One Repository contains the Jupiter One build and other builds</description>
            <platform>all</platform>
    </extension>
</addon>

<addon id="repository.urepo" name="uRepo Repository" version="1.0.0" provider-name="zag">
    <extension point="xbmc.addon.repository" name="uRepo Repository">
        <info compressed="false">http://www.urepo.org/zips/addons.xml</info>
        <checksum>http://www.urepo.org/zips/addons.xml.md5</checksum>
        <datadir zip="true">http://www.urepo.org/zips/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>uRepo.org Repository</summary>
        <description>uRepo.org is an indepent repository designed to be as simple as possible to use by users and developers who wish to upload Add-ons to a 3rd party website. We generally follow the same rules as the official Kodi repository and encourage developers to submit to that as well.</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <language></language>
        <license>GNU GENERAL PUBLIC LICENSE. Version 2</license>
        <forum></forum>
        <website>http://www.urepo.org</website>
        <source>https://github.com/zag2me/repository.urepo</source>
    </extension>
</addon>
</addons>
